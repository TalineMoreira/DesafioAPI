		
		for(int i = 0; i < nomesGruposAgentes.size(); i++) {
			if(!nomesGruposAgentes.get(i).getText().equals(grupoAgenteComercializador) &&
					!agentes.get(i).getText().equals(agenteComercializador) &&  
					!tipos.get(i).getText().equals(tipoCreditoDebito)){
				return false;
			}
		}
		
		return true;
	}
	
	
	
	
}
